{
  "version": "0.0.0",
  "name": "快递助手",
  "manifest_version": 2,
  "homepage_url": "https://github.com/minosss/express-plus",
  "description": "轻松快捷的管理你的快递，支持大部分快递公司、后台查询、桌面提示等功能。",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "browser_style": true
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "geolocation",
    "alarms",
    "storage",
    "notifications",
    "unlimitedStorage",
    "webRequestBlocking",
    "webRequest",
    "https://www.kuaidi100.com/*",
    "https://m.kuaidi100.com/*",
    "https://biz.trace.ickd.cn/*",
    "https://sp0.baidu.com/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
