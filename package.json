{
  "name": "express-plus",
  "private": true,
  "author": "mino",
  "license": "MIT",
  "keywords": [
    "express",
    "react",
    "web-extension",
    "chrome-extension"
  ],
  "scripts": {
    "dev": "parcel watch src/manifest.json",
    "build": "rm -rf dist && parcel build --no-source-maps src/manifest.json",
    "release": "pnpm build; VERSION=$(utc-version); echo $VERSION; dot-json dist/manifest.json version $VERSION; zip -r $VERSION.zip dist; mv $VERSION.zip release/;"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.13",
    "@mantine/hooks": "^6.0.13",
    "@mantine/notifications": "^6.0.13",
    "@tanstack/react-query": "^4.29.12",
    "dayjs": "^1.11.8",
    "jotai": "^2.1.1",
    "lucide-react": "^0.241.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxdb": "^14.14.0",
    "rxjs": "^7.8.1",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@parcel/config-webextension": "^2.9.2",
    "@parcel/core": "^2.9.2",
    "@parcel/reporter-bundle-analyzer": "^2.9.2",
    "@parcel/reporter-bundle-buddy": "^2.9.2",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/webextension-polyfill": "^0.10.0",
    "@yme/eslint-config": "^0.3.1",
    "@yme/tsconfig": "^1.0.0",
    "buffer": "^6.0.3",
    "dot-json": "^1.2.2",
    "eslint": "^8.41.0",
    "parcel": "^2.9.2",
    "process": "^0.11.10",
    "typescript": "^5.1.3",
    "utc-version": "^2.0.2"
  },
  "alias": {
    "process": "process/browser.js",
    "buffer": "buffer/index.js"
  }
}
