{
	"name": "express-plus",
	"private": true,
	"scripts": {
		"start": "NODE_ENV=development webpack-dev-server",
		"build": "NODE_ENV=production webpack",
		"update-version": "VERSION=$(utc-version); echo $VERSION; dot-json build/manifest.json version $VERSION; dot-json build/manifest.json content_security_policy --delete; zip -r $VERSION.zip build;"
	},
	"dependencies": {
		"@ant-design/icons": "^4.2.2",
		"@emotion/core": "^10.0.35",
		"@emotion/styled": "^10.0.27",
		"antd": "^4.6.3",
		"dayjs": "^1.8.33",
		"dexie": "^3.0.2",
		"ky": "^0.23.0",
		"lodash": "^4.17.20",
		"query-string": "^6.13.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-highlight-words": "^0.16.0",
		"react-router-dom": "^5.2.0",
		"react-use": "^15.3.3",
		"swr": "^0.3.0",
		"webextension-polyfill": "^0.6.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "2.x",
		"@typescript-eslint/parser": "2.x",
		"@yme/prettier-config": "^0.1.0",
		"@yme/webpack-core": "^0.4.6",
		"@yme/webpack-extension": "^0.4.6",
		"@yme/webpack-file": "^0.4.6",
		"@yme/webpack-less": "^0.4.6",
		"@yme/webpack-react": "^0.4.6",
		"babel-eslint": "10.x",
		"babel-loader": "^8.1.0",
		"babel-plugin-emotion": "^10.0.33",
		"babel-preset-react-app": "^9.1.2",
		"dot-json": "1.2.0",
		"eslint": "6.x",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-react-app": "^5.2.1",
		"eslint-plugin-flowtype": "4.x",
		"eslint-plugin-import": "2.x",
		"eslint-plugin-jsx-a11y": "6.x",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "7.x",
		"eslint-plugin-react-hooks": "2.x",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"prettier": "*",
		"stylelint": "^13.6.1",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-standard": "^20.0.0",
		"tailwindcss": "^1.7.3",
		"utc-version": "2.x",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix"
		],
		"*.{json,html}": [
			"prettier --write"
		],
		"*.{css,less}": [
			"stylelint"
		]
	},
	"prettier": "@yme/prettier-config",
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"engines": {
		"node": ">=12.x"
	}
}
